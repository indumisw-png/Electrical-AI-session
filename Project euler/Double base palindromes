num=list(map(str,list(range(int(input())))))
list_1=list(filter(lambda x:x==x[::-1],num))
list_4=list(filter(lambda x:int(x)%2!=0,list_1))
list_2=list(map(lambda x:bin(int(x))[2:],list_4))
list_3=list(filter(lambda x:x==x[::-1],list_2))
print(sum(list(map(int,list(map(lambda x:int(x,2),list_3))))))
